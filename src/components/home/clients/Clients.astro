---
// IMPORTING COMPONENTS
import ClientsItem from "./ClientsItem.astro"

const { data } = Astro.props
---
<section class="grid grid-cols-2 md:grid-cols-4 gap-y-16 md:gap-24 max-w-6xl mx-auto py-40">
    {

        data.images.map( ( item ) => {

            return(
                <ClientsItem
                    src={ item.file }
                    alt=""
                />
            )

        })

    }
</section>
